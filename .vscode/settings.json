                    "shouldRunCommands": true
                }
            ]
        },
        {
            "splitTerminals": [
                {
                    "name": "bin 1",
                    "commands": [
                        "cd bin/"
                    ],
                    "shouldRunCommands": true
                }
            ]
        },
        {
            "splitTerminals": [
                {
                    "name": "scripts 0",
                    "commands": [
                        "cd scripts"
                    ],
                    "shouldRunCommands": true
                },
                {
                    "name": "scripts 1",
                    "commands": [
                        "cd scripts"
                    ],
                    "shouldRunCommands": true
                }
            ]
        }
    ],
    "cSpell.enabled": true,
    "workbench.tree.indent": 16,
    "cmake.options.statusBarVisibility": "visible"
